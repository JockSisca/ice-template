    "@rematch/select": "^3.1.2",
    "antd": "^4.23.5",
    "axios": "^1.1.2",
    "build-plugin-fusion": "^0.1.23",
    "classnames": "^2.3.2",
    "css-loader": "^6.7.1",
    "immer": "^9.0.15",
    "mockjs": "^1.1.0",
    "npm": "^8.19.2",
    "postcss-loader": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^18.8.3",
    "@types/postcss-flexbugs-fixes": "^4.2.1",
    "@types/postcss-import": "^14.0.0",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/postcss-url": "^8.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "autoprefixer": "^10.4.12",
    "build-plugin-icestark": "^2.5.3",
    "build-plugin-ignore-style": "^0.1.2",
    "cssnano": "^5.1.13",
    "cssnano-preset-advanced": "^5.3.8",
    "eslint": "^7.30.0",
    "ice.js": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.0.0",
    "postcss-preset-env": "^7.8.2",
    "postcss-url": "8.0.0",
    "stylelint": "^13.7.2",
    "thread-loader": "^3.0.4"
  },
  "scripts": {
    "tsc": "tsc --project config/tsconfig.json",
    "tsc:watch": "tsc --project config/tsconfig.json -w",
    "start": "npm run tsc && icejs start --config .build.config/config.js --mode dev",
    "build": "npm run tsc && icejs build --config .build.config/config.js --mode prod",
    "build:dbg": "npm run tsc && icejs build --config .build.config/config.js --mode dbg",
    "tree": "node tree.js -i \"node_modules|.idea|.git|dist|t|medias|images|.ice|mock|.build.config|build\" -r \".md|.json\" -e tree.md",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "start:w": " npm-run-all --parallel tsc:watch start",
    "build:analyzer": "npm run tsc && icejs build --config .build.config/config.js --analyzer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/react-materials/tree/master/scaffolds/simple"
  },
  "private": true,
  "originTemplate": "@alifd/scaffold-simple"
}